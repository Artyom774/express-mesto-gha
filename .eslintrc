{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-essential",
      "airbnb-base"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "plugins": [
      "vue"
  ],
  "rules": {
    "no-underscore-dangle": ["error",{"allow":["_id"]}]
  }
}